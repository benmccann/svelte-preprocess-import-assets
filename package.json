{
	"name": "svelte-preprocess-import-assets",
	"description": "Import assets in markup",
	"version": "0.1.2",
	"license": "MIT",
	"author": "Bjorn Lu",
	"types": "./dist/index.d.ts",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"files": [
		"dist"
	],
	"homepage": "https://github.com/bluwy/svelte-preprocess-import-assets",
	"repository": {
		"type": "git",
		"url": "https://github.com/bluwy/svelte-preprocess-import-assets.git"
	},
	"bugs": {
		"url": "https://github.com/bluwy/svelte-preprocess-import-assets/issues"
	},
	"keywords": [
		"svelte",
		"preprocess",
		"import",
		"assets"
	],
	"scripts": {
		"build": "tsc && tsup src/index.ts --dts --format esm,cjs",
		"test": "esno test/index.ts",
		"test:update": "esno test/index.ts -u",
		"format": "prettier -w {src,test}/**/*.{ts,svelte}",
		"prepublishOnly": "rm -rf dist && pnpm build"
	},
	"dependencies": {
		"magic-string": "^0.25.7"
	},
	"peerDependencies": {
		"svelte": "^3.37.0"
	},
	"devDependencies": {
		"@types/node": "^14.14.41",
		"esno": "^0.5.0",
		"prettier": "^2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"svelte": "^3.37.0",
		"tsup": "^4.9.2",
		"typescript": "^4.2.4",
		"uvu": "^0.5.1"
	}
}